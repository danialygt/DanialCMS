@model IEnumerable<System.Security.Claims.Claim>


<table class="table table-bordered">
    <tr>
        <th>subject</th>
        <th>issuer</th>
        <th>type</th>
        <th>value</th>
    </tr>
    @if (Model.Any())
    {
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Subject.Name</td>
                <td>@item.Issuer</td>
                <td identity-claim-type="@item.Type"></td>
                <td>@item.Value</td>
            </tr>
        }
    }
    else
    {
        <p>nabod chizi!!</p>
    }
</table>